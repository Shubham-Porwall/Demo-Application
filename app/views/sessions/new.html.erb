<center>
  <h1> Welcome to Log-In Page </h1>
  <%= render "shared/flash_message" %>
  <%= form_with models: @customer, url: sessions_create_path, method: :post do |form| %>
    <div>
      <%= form.label :email %><br>
      <%= form.text_field :email %>
    </div>

    <div>
      <%= form.label :password %><br>
      <%= form.password_field :password %><br>
    </div><br>

    <div>
      <%= form.submit "Login"%>
    </div>
  <% end %>
</center>
